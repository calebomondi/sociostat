{
    "builds": [{
        "src": "dashboard/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb",
            "runtime": "python3.12",
            "maxDuration": 60
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dashboard/wsgi.py"
        }
    ]
}